{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "class-methods-use-this": "off",
        "import/extensions": "off",
        "no-prototype-builtins": "off",
        "linebreak-style": "off"
      }
}
